{
  "name": "my-track-expenses",
  "private": true,
  "version": "1.0.0",
  "description": "A cross-platform expense tracking app built with web technologies",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "cap:sync": "cap sync",
    "cap:copy": "cap copy",
    "cap:open:android": "cap open android",
    "build:android": "npm run build && npm run cap:sync",
    "android:build:debug": "cd android && ./gradlew assembleDebug",
    "android:build:release": "cd android && ./gradlew assembleRelease"
  },
  "devDependencies": {
    "vite": "^7.2.4"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4"
  }
}
