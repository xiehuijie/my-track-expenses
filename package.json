{
  "name": "my-track-expenses",
  "private": true,
  "version": "1.0.0",
  "description": "A cross-platform expense tracking app built with Vue 3 + TypeScript",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "cap:sync": "cap sync",
    "cap:copy": "cap copy",
    "cap:open:android": "cap open android",
    "cap:run:android": "cap run android",
    "build:android": "npm run build && npm run cap:sync",
    "android:build:debug": "cd android && ./gradlew assembleDebug",
    "android:build:release": "cd android && ./gradlew assembleRelease",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/sql.js": "^1.4.9",
    "@unocss/preset-wind4": "^66.5.10",
    "@unocss/reset": "^66.5.10",
    "@unocss/vite": "^66.5.10",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.6.2",
    "jsdom": "^27.2.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "unocss": "^66.5.10",
    "vite": "^7.2.4",
    "vitest": "^4.0.15",
    "vue-tsc": "^3.1.5"
  },
  "dependencies": {
    "@capacitor-community/sqlite": "^7.0.2",
    "@capacitor/android": "^7.4.4",
    "@capacitor/app": "^7.1.0",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/haptics": "^7.0.2",
    "@capacitor/status-bar": "^7.0.3",
    "@mdi/font": "^7.4.47",
    "capacitor-plugin-safe-area": "^4.0.3",
    "localforage": "^1.10.0",
    "pinia": "^3.0.4",
    "reflect-metadata": "^0.2.2",
    "sql.js": "^1.13.0",
    "typeorm": "^0.3.28",
    "vue": "^3.5.25",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.6.3",
    "vuetify": "^3.11.2"
  }
}
